<?xml version="1.0"?>

<!-- For more information on using Web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=301874 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <!--
    In the example below, the "SetAttributes" transform will change the value of
    "connectionString" to use "ReleaseSQLServer" only when the "Match" locator
    finds an attribute "name" that has a value of "MyDB".-->
    <connectionStrings>
      <add name="MyConnection"
        connectionString="server=srv-sql004\SQLMUTU; user id=motoechange; password=*****; MultipleActiveResultSets=True; database=motoechange; pooling=false" providerName="System.Data.SqlClient"
        xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    </connectionStrings>
  
  
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
  
  <system.net>
    <mailSettings>
      <smtp xdt:Transform="Remove"></smtp>
      <smtp from="admin@moto-echange.fr" deliveryMethod="Network" xdt:Transform="Insert" >
        <network host="smtp.moto-echange.fr" port="587" userName="admin@moto-echange.fr" password="*****" enableSsl="false" defaultCredentials="false" />
      </smtp>
  </mailSettings>
  </system.net>

</configuration>
